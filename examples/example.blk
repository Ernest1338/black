// print function call
print("hello, world")

// printing multiple things
print("hello", 123)

// variable declaration
let asdf = "hello, sailor"

// print binary expression
print(1 + 1 + 67)

// printing variable
print(asdf)

// creating a variable containing bin expr and printing it
let qwer = 1 + 1
print(qwer)

// adding two variables together
let a = 1
let b = 2
let c = a + b // inline comment
print(c, a + b)
